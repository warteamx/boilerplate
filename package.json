{
  "name": "react-starter",
  "version": "6.0.0",
  "proxy": "http://localhost:5000/",
  "homepage": "https://boilerplate-mern.ey.r.appspot.com/",
  "private": true,
  "type": "module",
  "dependencies": {
    "apollo-server-express": "^2.19.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "firebase-admin": "^9.10.0",
    "google-auth-library": "^7.0.2",
    "graphql": "^15.4.0",
    "graphql-tools": "^7.0.1",
    "lodash.assign": "^4.2.0",
    "lodash.values": "^4.3.0",
    "mongoose": "^5.10.9",
    "react-firebaseui": "^5.0.2"
  },
  "scripts": {
    "startDEV:server": "cd server && ts-node-dev --respawn --transpile-only ./server.ts && wait-on tcp:5000",
    "startDEV:app": "react-scripts start",
    "startDEV": "run-p --race startDEV:server startDEV:app",
    "start": "node server.js",
    "deploy": "gcloud app deploy PROD/app.yaml",
    "build": "react-scripts build && mkdir PROD && mv build PROD/build && tsc -p server/ && cp package.json PROD/package.json && cp app.yaml PROD/app.yaml ",
    "test": "react-scripts test ",
    "test-server": "npx mocha ./server/softonic/*.test.ts",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'PROD/build/static/js/*.js'",
    "create-env": "printenv > .env"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@react-google-maps/api": "^1.13.0",
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/express": "^4.17.8",
    "@types/googlemaps": "^3.40.3",
    "@types/jest": "^24.0.0",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-syntax-highlighter": "^13.5.0",
    "chai": "^4.3.4",
    "classnames": "^2.2.6",
    "clsx": "^1.1.1",
    "create-env": "^1.1.1",
    "firebase": "^8.7.1",
    "firebaseui": "^4.8.0",
    "mocha": "^9.0.3",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-fetch-hook": "^1.8.5",
    "react-google-login": "^5.2.2",
    "react-hook-form": "^6.15.4",
    "react-markdown": "^5.0.2",
    "react-query": "^3.18.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-syntax-highlighter": "^15.3.0",
    "react-transition-group": "^4.4.1",
    "recharts": "^2.0.8",
    "redux-devtools": "^3.7.0",
    "redux-thunk": "^2.3.0",
    "remark-gfm": "^1.0.0",
    "source-map-explorer": "^2.5.0",
    "supertest": "^6.1.4",
    "ts-node-dev": "^1.0.0-pre.64",
    "typescript": "~3.8.0"
  }
}
